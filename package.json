{
	"name": "zaptochar",
	"displayName": "zap-to-char",
	"description": "zap-to-char for Visual Studio Code",
	"publisher": "sharapeco",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/sharapeco/vscode-zap-to-char/"
	},
	"icon": "assets/icon.png",
	"galleryBanner": {
		"color": "#3a3d41",
		"theme": "dark"
	},
	"engines": {
		"vscode": "^1.46.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"emacs",
		"xyzzy",
		"zap",
		"cursor"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "zaptochar.forward",
				"title": "Zap to char: Jump forward"
			},
			{
				"command": "zaptochar.backward",
				"title": "Zap to char: Jump backward"
			},
			{
				"command": "zaptochar.deleteForward",
				"title": "Zap to char: Delete forward"
			},
			{
				"command": "zaptochar.deleteBackward",
				"title": "Zap to char: Delete backward"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.46.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
